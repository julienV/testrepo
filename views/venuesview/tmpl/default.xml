<?xml version="1.0" encoding="utf-8"?>
<metadata>
	<layout title="Venuesview Layout">
		<message>
			<![CDATA[VENUESVIEW DESC]]>
		</message>
	</layout>
	<state>
		<name>Venuesview Layout</name>
		<description>VENUESVIEW DESC</description>
		<params>
			<param name="header" type="text" size="30" default="" label="Page Title" description="PARAMPAGETITLE" />
			<param name="page_title" type="radio" default="1" label="Page Title" description="Show/Hide the pages Title">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
			<param name="@spacer" type="spacer" default="" label="" description="" />
			<param name="display_venues_num" type="list" default="15" label="VENUE NUMBER" description="VENUE NUMBER TIP">
				<option value="5">5</option>
				<option value="10">10</option>
				<option value="15">15</option>
				<option value="20">20</option>
				<option value="25">25</option>
				<option value="30">30</option>
				<option value="50">50</option>
			</param>
			<param name="@spacer" type="spacer" default="" label="" description="" />
			<param name="display_num" type="list" default="15" label="EVENT NUMBER" description="EVENT NUMBER TIP">
				<option value="5">5</option>
				<option value="10">10</option>
				<option value="15">15</option>
				<option value="20">20</option>
				<option value="25">25</option>
				<option value="30">30</option>
				<option value="50">50</option>
			</param>
		</params>
		<advanced>
			<param name="menu_image" type="imagelist" directory="/images/stories" hide_default="1" default="" label="Menu Image" description="PARAMMENUIMAGE" />
			<param name="secure" type="radio" default="0" label="SSL Enabled" description="PARAMSECURE">
				<option value="-1">Off</option>
				<option value="0">Ignore</option>
				<option value="1">On</option>
			</param>
			<param name="icons" type="radio" default="1" label="Show Icons" description="Show the icons or use alternative text">
				<option value="0">Hide</option>
				<option value="1">Show</option>
			</param>
		</advanced>
	</state>
</metadata>