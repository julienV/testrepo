			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>		
		
		<param type="spacer"/>
		
		<param name="showatte" type="radio" default="1" label="COM_EVENTLIST_SETTINGS_FRONT_TABLE_SHOW_ATTENDEES" description="COM_EVENTLIST_SETTINGS_FRONT_TABLE_SHOW_ATTENDEES_TIP" >
