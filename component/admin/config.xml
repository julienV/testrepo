		<param name="showtime" type="radio" default="1" label="COM_EVENTLIST_SETTINGS_FRONT_TABLE_WIDTH_DISPLAY_TIME" description="COM_EVENTLIST_SETTINGS_FRONT_TABLE_WIDTH_DISPLAY_TIME_TIP">
